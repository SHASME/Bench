package capgemini;

public class CountNumber {
public static void main(String[] args) {
	 String str ="62782828278229289";
	 int count=0;
	 for (int i = 0; i < str.length(); i++) {
		if(str.charAt(i)!=' ')
			count++;
	}
	 System.out.println("count the number of values in the  given string"+count);
}
}
